<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %> 
</head>
<body >
  <%- include("../partials/nav2")  %> 
  <div class="container mt-3">
    <div class="row">
        </div>
        <div class="row position-static">
          <div class="col-md-6 z-3 position-absolute top-50 start-50 translate-middle">
            <table class="table table-striped table-bordered table-responsive ">
              <thead class="table-bordered ">
              </thead>
              <tbody>
               <% Tickets.forEach(ticket => { %>
                  <tr>
                    <td><%= ticket.dataValues.name %> </td>
                    <td><%= ticket.dataValues.plate_number %> </td>
                    <td><%= ticket.dataValues.fromm %> </td>
                    <td> ->  </td>
                    <td><%= ticket.dataValues.too %> </td>
                    <td><%= ticket.dataValues.ticket_price %> </td>
                    <td>
                        <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                         <a class="btn btn-sm btn-danger" href="/users/ticket_create">BUY</a> 
                        </div>
                    </td>
                  </tr>
               <% }) %>
              </tbody>
            </table>
          </div>
        </div>
         </div>
      </div>
    </div>
</body>
</html>